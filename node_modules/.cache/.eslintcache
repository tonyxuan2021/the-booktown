CartContext\";\nimport { useContext } from \"react\";\n\nconst Header = () => {\n  const { items } = useContext(CartContext);\n  // console.log(items);\n\n  return (\n    <div className=\"header__wrapper\">\n      <div className=\"header--top\">\n        <Link to=\"/\" className=\"header__link\">\n          <img src={logo} className=\"header__img\"></img>\n        </Link>\n        <div className=\"header--right__wrapper\">\n          <div className=\"header--right\">\n            <p>Login</p>\n            <FontAwesomeIcon icon={faUser} />\n          </div>\n          <div className=\"header--btm\">\n            <Link to=\"/checkout\" className=\"header--right\">\n              <p>Cart</p>\n              <FontAwesomeIcon icon={faCartShopping} className=\"header__cart\" />\n              <span className=\"header__cart__num\">{items.length}</span>\n            </Link>\n          </div>\n        </div>\n      </div>\n      <div className=\"header__bar\">\n        <Link to=\"/\" className=\"header__home\">\n          <p>Home</p>\n        </Link>\n        <Link to=\"/used\" className=\"header__used\">\n          <p>Sell Used</p>\n        </Link>\n      </div>\n      <div className=\"search__wrapper\">\n        <input\n          className=\"search__input\"\n          placeholder=\"Search over millions of books...\"\n        ></input>\n        <FontAwesomeIcon icon={faSearch} className=\"search__icon\" />\n      </div>\n    </div>\n  );\n};\n\nexport default Header;\n",[],"/Users/xuanye/Documents/BrainStation/Project_BrainStation/The BookTown/frontend/src/components/Hero/Hero.js",["82"],[],"import React from \"react\";\nimport \"./Hero.scss\";\nimport hero from \"../../assets/hero.jpg\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faQuoteLeft } from \"@fortawesome/free-solid-svg-icons\";\nimport { faQuoteRight } from \"@fortawesome/free-solid-svg-icons\";\n\nconst Hero = () => {\n  return (\n    <div className=\"hero__main__wrapper\">\n      <div className=\"hero__quote__wrapper\">\n        <FontAwesomeIcon icon={faQuoteLeft} className=\"hero__quote--left\" />\n        <blockquote className=\"hero__quote__text\">\n          The more that you read, the more things you will know. The more that\n          you learn, the more places youâ€™ll go. --{\" \"}\n          <div className=\"hero__quote__author\">Dr. Seuss</div>\n        </blockquote>\n        <FontAwesomeIcon icon={faQuoteRight} className=\"hero__quote--right\" />\n      </div>\n      <div className=\"hero__img__wrapper\">\n        <img src={hero} className=\"hero__img\"></img>\n      </div>\n    </div>\n  );\n};\n\nexport default Hero;\n","/Users/xuanye/Documents/BrainStation/Project_BrainStation/The BookTown/frontend/src/components/Category/Category.js",[],[],"/Users/xuanye/Documents/BrainStation/Project_BrainStation/The BookTown/frontend/src/components/Card/Card.js",["83","84","85","86"],[],"import React, { useContext, useEffect, useState } from \"react\";\nimport Button from \"../Button/Button\";\nimport \"./Card.scss\";\nimport axios from \"axios\";\nimport Rating from \"../Rating/Rating\";\nimport CartContext from \"../../CartContext\";\n\nconst Card = ({ dataObj, isbn }) => {\n  const [imgUrl, setImgUrl] = useState(\"\");\n  const [data2, setData2] = useState(\"\");\n  const [cart, setCart] = useState([]);\n\n  const { addToCart } = useContext(CartContext);\n\n  useEffect(() => {\n    axios\n      .get(`https://www.googleapis.com/books/v1/volumes?q=isbn:${isbn}`)\n      .then((data2) => {\n        setImgUrl(data2.data.items[0].volumeInfo.imageLinks.thumbnail);\n        setData2(data2);\n      });\n  }, []);\n\n  if (!data2) {\n    return (\n      <section>\n        <p>... Loading your bookData ...</p>\n      </section>\n    );\n  }\n\n  let ratingScore = data2.data.items[0].volumeInfo.averageRating;\n  let ratingCount = data2.data.items[0].volumeInfo.ratingsCount;\n  let bookImgUrl = data2.data.items[0].volumeInfo.imageLinks.thumbnail;\n\n  return (\n    <div className=\"card__wrapper\">\n      <img src={imgUrl} className=\"card__img\"></img>\n      <p className=\"card__name\">{dataObj.title}</p>\n      <p className=\"card__author\">{dataObj.author}</p>\n\n      {dataObj.price === \"0.00\" ? (dataObj.price = 19.99) : dataObj.price}\n      <div className=\"card__review__wrapper\">\n\n        <Rating value={ratingScore} text={ratingCount} />\n      </div>\n\n      <Button addToCart={addToCart} dataObj={dataObj} bookImgUrl={bookImgUrl}/>\n\n    </div>\n  );\n};\n\nexport default Card;\n","/Users/xuanye/Documents/BrainStation/Project_BrainStation/The BookTown/frontend/src/components/Button/Button.js",[],[],"/Users/xuanye/Documents/BrainStation/Project_BrainStation/The BookTown/frontend/src/components/Footer/Footer.js",[],[],"/Users/xuanye/Documents/BrainStation/Project_BrainStation/The BookTown/frontend/src/pages/HomePage.js",["87"],[],"import \"../App.scss\";\nimport React, { Component, useContext } from \"react\";\nimport Hero from \"../components/Hero/Hero\";\nimport Category from \"../components/Category/Category\";\nimport Card from \"../components/Card/Card\";\nimport Footer from \"../components/Footer/Footer\";\nimport axios from \"axios\";\nimport \"../components/Card/Card.scss\";\nimport uniqid from \"uniqid\";\n\nconst API_KEY_NYT = process.env.REACT_APP_API_KEY_NYT;\n\nclass HomePage extends Component {\n  state = {\n    bookData: [],\n    selectedBook: null,\n    bookImg: {},\n    results: [],\n  };\n\n  componentDidMount() {\n    axios\n      .get(\n        `https://api.nytimes.com/svc/books/v3/lists.json?api-key=${API_KEY_NYT}&list=Hardcover Fiction`\n      )\n      .then((data) => {\n        // console.log(\"book\", data);\n        this.setState({\n          bookData: data.data.results,\n          selectedBook: data.data.results[0],\n        });\n      });\n  }\n\n  render() {\n    if (this.state.bookData.length === 0 || !this.state.bookImg) {\n      return (\n        <section>\n          <p>... Loading your bookData ...</p>\n        </section>\n      );\n    }\n\n    return (\n      <div>\n        <Hero />\n        <Category />\n        <h3 className=\"card__title\">Best Sellers</h3>\n        <div className=\"card__each__wrapper\">\n          {this.state.bookData.map((bookData) => (\n            <div className=\"card__main__wrapper\" key={uniqid()}>\n                <Card\n                  // dataInitObj={this.state.selectedBook}\n                  dataObj={bookData.book_details[0]}\n                  isbn={bookData.isbns[0].isbn13}\n                />\n            </div>\n          ))}\n        </div>\n        <Footer />\n      </div>\n    );\n  }\n}\n\nexport default HomePage;\n","/Users/xuanye/Documents/BrainStation/Project_BrainStation/The BookTown/frontend/src/components/Rating/Rating.js",[],[],"/Users/xuanye/Documents/BrainStation/Project_BrainStation/The BookTown/frontend/src/CartContext.js",[],[],"/Users/xuanye/Documents/BrainStation/Project_BrainStation/The BookTown/frontend/src/components/Checkout/Checkout.js",["88","89","90"],[],"/Users/xuanye/Documents/BrainStation/Project_BrainStation/The BookTown/frontend/src/pages/SellUsedPage.js",["91"],[],"import React, { Component } from \"react\";\nimport \"./SellUsedPage.scss\";\nimport \"../components/Button/Button.scss\";\nimport axios from \"axios\";\nimport { Link } from \"react-router-dom\";\nimport uniqid from \"uniqid\";\n\nclass SellUsedPage extends Component {\n  state = {\n    bookdata: [],\n  };\n\n  componentDidMount() {\n    axios.get(`http://localhost:5050/usedbook/`).then((response) => {\n      const booksResult = response.data;\n      this.setState({ bookdata: booksResult });\n    });\n  }\n\n  render() {\n    // console.log(this.state.bookdata)\n    return (\n      <div>\n        <div className=\"upload__header\">\n          <h1>Used Book List</h1>\n          <Link to=\"/used/upload\" className=\"upload__button\">\n            <button>UPLOAD</button>\n          </Link>\n       